{
  "name": "it-job-finder-client",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --open --config webpack.config.js",
    "build": "webpack --mode production",
    "dev": "webpack serve --open --config webpack.config.js --mode development",
    "deploy": "git add . && git commit -m heroku && git push heroku master",
    "lint": "npx eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "lint:check": "npx eslint --ext .js,.jsx,.ts,.tsx .",
    "format": "npx prettier --write ./src/**/**.+(js|jsx|ts|tsx|json|css|scss)",
    "format:check": "npx prettier --check ./src/**/**.+(js|jsx|ts|tsx|json|css|scss)",
    "test": "npx jest --coverage --watchAll=false --coverageReporters=text-summary --env=jsdom",
    "test:coverage": "npx jest --coverage --watchAll=false --env=jsdom",
    "test:watch": "npx jest --env=jsdom"
  },
  "author": "Bart≈Çomiej Hildebrandt",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx|json|css|scss)": [
      "prettier --write",
      "git add"
    ],
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix",
      "git add"
    ]
  },
